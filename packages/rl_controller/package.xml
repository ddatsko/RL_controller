<?xml version="1.0"?>
<package format="2">

  <name>rl_controller</name>
  <version>1.0.0</version>
  <description>Reinforcement Learning controller</description>

  <author email="datskden@fel.cvut.cz">Denys Datsko</author>
  <maintainer email="datskden@fel.cvut.cz">Denys Datsko</maintainer>

  <license>BSD 3-Clause</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>cmake_modules</depend>
  <depend>pluginlib</depend>
  <depend>roscpp</depend>
  <depend>nodelet</depend>
  <depend>mrs_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>mrs_lib</depend>
  <depend>std_srvs</depend>
